<% if (typeof detail!='undefined' && detail) { %>
    <div class="row mt-2">
        <div class="col-12">
            <h3><%=detail.name%></h3>
            <p>twitter username : <%=detail.twitterUsername%></p>
        </div>
        <div class="col-12">
        <div class="joindiv">
            <a href="https://ton-connect.contributiondao.com/connect?redirect_group_id=<%=detail.id%>"><button class="joinbtn mx-2">Join community</button></a>
        </div>
        </div>
    </div>
    <div class="row mt-3">
        <div class="col-12">
            <img class="detailimg" src="<%=detail.avatar && detail.avatar!=''?detail.avatar:'/images/no-image.png'%>">
        </div>
    </div>
    <div class="wallet mt-2">
        <h4>SBTS</h4>
         <div class="row row-cols-1 row-cols-md-2 row-cols-lg-3">
            <% if (typeof detail.sbts!='undefined' && detail.sbts && detail.sbts.length > 0) { %>
                <% detail.sbts.forEach(function(data){ %>
                    <div class="col">
                        <div class="eachsbt bg-white p-3">
                            <div>
                                telegram group id : <%=data.telegramGroupId%>
                            </div>
                            <a href="https://testnet.tonscan.org/address/<%data.contractAddress%>">
                                <button class="goto mt-3">testnet</button>
                            </a>
                        </div>
                    </div>
                <% }); %>
             <% } %>
         </div>
    </div>
