<div class="row mt-3">
    <div class="col-12">
        <img class="bgimg" src="/images/telegram-bg.jpg">
    </div>
</div>
<% if (typeof alldata!='undefined' && alldata && alldata.length>0) { %>
    <div class="row detailgroup mt-5">
        <%alldata.forEach(function(itm){%>
            <div class="col-6 col-lg-3 p-2">
                <div class="eachgroup">
                    <a href="/detail/<%=itm.id%>">
                        <img class="groupimgtop" src="<%=itm.avatar&&itm.avatar!=''?itm.avatar:'/images/no-image.png'%>" alt="Card image cap">
                        <div class="groupinfo m-3">
                        <h5 class="grouptitle"><%=itm.name%></h5>
                        <p class="grouptext t7"><%=itm.twitterUsername%></p>
                        </div>
                    </a>
                </div>
            </div>
        <%});%>
    </div>
<%}%>